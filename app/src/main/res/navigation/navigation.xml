<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation.xml"
    app:startDestination="@id/loginFragment">
    <fragment
        android:id="@+id/loginFragment"
        android:name="edu.itesm.tiendaperroingles.view.LoginFragment"
        android:label="LoginFragment" >
        <action
            android:id="@+id/action_loginFragment_to_mainMenu1Fragment"
            app:destination="@id/mainMenu1Fragment" />
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
    </fragment>
    <fragment
        android:id="@+id/mainMenu1Fragment"
        android:name="edu.itesm.tiendaperroingles.view.MainMenu1Fragment"
        android:label="fragment_main_menu1"
        tools:layout="@layout/fragment_main_menu1" >
        <action
            android:id="@+id/action_mainMenu1Fragment_to_appointments5Fragment"
            app:destination="@id/appointments5Fragment" />
        <action
            android:id="@+id/action_mainMenu1Fragment_to_catalogueListFragment"
            app:destination="@id/catalogueListFragment" />
    </fragment>
    <fragment
        android:id="@+id/productDetail3Fragment"
        android:name="edu.itesm.tiendaperroingles.view.ProductDetail3Fragment"
        android:label="fragment_product_detail3"
        tools:layout="@layout/fragment_product_detail3" >
        <action
            android:id="@+id/action_productDetail3Fragment_to_paymentView4Fragment"
            app:destination="@id/paymentView4Fragment" />
        <argument android:name="catalogueItems"
            app:argType="edu.itesm.tiendaperroingles.model.CatalogueModel" />
    </fragment>
    <fragment
        android:id="@+id/paymentView4Fragment"
        android:name="edu.itesm.tiendaperroingles.view.PaymentView4Fragment"
        android:label="fragment_payment_view4"
        tools:layout="@layout/fragment_payment_view4" />
    <fragment
        android:id="@+id/appointments5Fragment"
        android:name="edu.itesm.tiendaperroingles.view.Appointments5Fragment"
        android:label="fragment_appointments5"
        tools:layout="@layout/fragment_appointments5" />
    <fragment
        android:id="@+id/registerFragment"
        android:name="edu.itesm.tiendaperroingles.view.RegisterFragment"
        android:label="fragment_register" >
        <action
            android:id="@+id/action_registerFragment_to_loginFragment"
            app:popUpTo="@+id/loginFragment"/>
    </fragment>
    <fragment
        android:id="@+id/catalogueListFragment"
        android:name="edu.itesm.tiendaperroingles.view.recyclerview.list.CatalogueListFragment"
        android:label="fragment_catalogue_list"
        tools:layout="@layout/fragment_catalogue_list" >
        <action
            android:id="@+id/action_catalogueListFragment_to_productDetail3Fragment"
            app:destination="@id/productDetail3Fragment" />
    </fragment>
</navigation>